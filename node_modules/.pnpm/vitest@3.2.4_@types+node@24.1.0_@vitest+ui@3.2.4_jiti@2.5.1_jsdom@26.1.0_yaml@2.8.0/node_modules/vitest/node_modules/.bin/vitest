#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sangsoo/WebstormProjects/hanghae/front_6th_chapter2-1/node_modules/.pnpm/vitest@3.2.4_@types+node@24.1.0_@vitest+ui@3.2.4_jiti@2.5.1_jsdom@26.1.0_yaml@2.8.0/node_modules/vitest/node_modules:/Users/sangsoo/WebstormProjects/hanghae/front_6th_chapter2-1/node_modules/.pnpm/vitest@3.2.4_@types+node@24.1.0_@vitest+ui@3.2.4_jiti@2.5.1_jsdom@26.1.0_yaml@2.8.0/node_modules:/Users/sangsoo/WebstormProjects/hanghae/front_6th_chapter2-1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sangsoo/WebstormProjects/hanghae/front_6th_chapter2-1/node_modules/.pnpm/vitest@3.2.4_@types+node@24.1.0_@vitest+ui@3.2.4_jiti@2.5.1_jsdom@26.1.0_yaml@2.8.0/node_modules/vitest/node_modules:/Users/sangsoo/WebstormProjects/hanghae/front_6th_chapter2-1/node_modules/.pnpm/vitest@3.2.4_@types+node@24.1.0_@vitest+ui@3.2.4_jiti@2.5.1_jsdom@26.1.0_yaml@2.8.0/node_modules:/Users/sangsoo/WebstormProjects/hanghae/front_6th_chapter2-1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
